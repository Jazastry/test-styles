<input 
class="{{input}}"
{{#if id}}
  id="{{id}}"
{{/if}}
{{#if inputType}}
  type="{{inputType}}"
{{/if}}
{{#if inputValue}}
  value="{{inputValue}}"
{{/if}}
{{#if name}}
  name="{{name}}"
{{/if}}
{{#if placeholder}}
  placeholder="{{placeholder}}"
{{/if}}
{{#if disabled}}
  disabled
{{/if}}
{{#if checked}}
  checked
{{/if}}
>