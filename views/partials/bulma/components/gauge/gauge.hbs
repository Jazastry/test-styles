  strokeLinejoin="round"
  strokeMiterlimit="2"
  clipRule="evenodd"
  viewBox="0 0 200 200"
  style="height: inherit; width: inherit;"
>
  <defs>
    <clipPath id="donut-path" transform="matrix(1.33333,0,0,1.33333,-486.941,-89.8718)">
